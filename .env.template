# FB Nova - Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git (it's in .gitignore)

# Azure Storage Account Connection String
# Format: DefaultEndpointsProtocol=https;AccountName=<account>;AccountKey=<key>;EndpointSuffix=core.windows.net
STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=stsynfbnovadev;AccountKey=YOUR_STORAGE_ACCOUNT_KEY_HERE;EndpointSuffix=core.windows.net

# Azure Storage Account Key (used for COPY INTO command authentication)
# This is the same key as in STORAGE_CONNECTION_STRING, but extracted separately
STORAGE_KEY=YOUR_STORAGE_ACCOUNT_KEY_HERE

# Your Azure AD username (email address)
# Used for Azure AD Interactive authentication to Synapse databases
USERNAME=your.email@company.com

